<?xml version="1.0"?>
<extension key="osdi-client" type="module">
  <file>osdi_client</file>
  <name>OSDI Client</name>
  <description>Sync CiviCRM records with remote OSDI services including Action Network</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Noah Miller, Lemniscus</author>
    <email>nm@lemnisc.us</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/lemniscus/osdi-client</url>
    <url desc="Documentation">http://FIXME</url>
    <url desc="Support">http://FIXME</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2020-11-29</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments>This is a new, undeveloped module</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
    <psr0 prefix="CRM_" path="."/>
  </classloader>
  <civix>
    <namespace>CRM/OSDI</namespace>
    <format>22.10.0</format>
  </civix>
  <mixins>
    <mixin>ang-php@1.0.0</mixin>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
  </mixins>
</extension>
