<?xml version="1.0"?>
<extension key="osdi-client" type="module">
  <file>osdi_client</file>
  <name>OSDI Client</name>
  <description>Sync CiviCRM records with remote OSDI services including Action Network</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Noah Miller, Lemniscus</author>
    <email>nm@lemnisc.us</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/lemniscus/osdi-client</url>
    <url desc="Documentation">https://github.com/lemniscus/osdi-client</url>
    <url desc="Support">https://lemnisc.us</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-08-07</releaseDate>
  <version>1.2.2</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.75.1</ver>
  </compatibility>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
    <psr0 prefix="CRM_" path="."/>
  </classloader>
  <civix>
    <namespace>CRM/OSDI</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>ang-php@1.0.0</mixin>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
    <mixin>entity-types-php@1.0.0</mixin>
  </mixins>
  <upgrader>CRM_OSDI_Upgrader</upgrader>
</extension>
